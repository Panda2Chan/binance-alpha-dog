{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "preserve",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment",
    "moduleResolution": "node",
    "types": ["vite/client"],
    "allowSyntheticDefaultImports":true,
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "src/**/*.vue", "shims-jsx.d.ts"]
}
